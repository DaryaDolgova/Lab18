using namespace std;

#include <iostream>

int main() 
{

	int N, M, i, j, K, s, p;
	cout << "M="; cin >> M;
	cout << "N="; cin >> N;
	int A[M][N];

for (i = 0; i < M; i++)
{
for (j = 0; j < N; j++)
{
cin >> A[i][j];
}
cout << endl;
}
	cout << "K="; cin >> K;
	s = 0; p = 1;
	for (i = 1; i < M + 1; i++) {
		for (j = 1; j < N + 1; j++) {
			if (i == K) {
				s += A[i][j];
				p *= A[i][j];
			}
		}
	}
	cout << "Summa=" << s << endl;
	cout << "Proizvedenie=" << p << endl;
	return 0;
}
