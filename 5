using namespace std;

#include <iostream>

int main() 
{

	int s, N, M, i, x, w, j;
	
	cout << "M="; cin >> M;
	cout << "N="; cin >> N;
	s = M * N;
	int* A = new int[s];
	double* B = new double[N];
	
	for (j = 1; j <= s; j++) 
	{
		cin >> A[j];
	}
	x = 0; w = 0;
	for (i = 1; i <= N; i++) 
	{
		for (j = i; j <= s; j += N) 
		{
			if (A[j] % 2 != 0) { x++; }
		}
			B[i] = x;
			x = 0;
	}
	for (i = 1; i <= N; i++) {
		cout << i  << B[i] << endl;
	}
	for (i = 1; i <= N; i++) {
		if (B[i] == M) { w = i; }
	}
	if (w == 0) { cout << "0" << endl; }
	else { cout << "stolbec - " << w << endl; }
	return 0;
}
