using namespace std;

#include <iostream>

int main() 
{
	int N, M, i, j, K, s, p, m, w, z;

	cout << "M="; cin >> M;
	cout << "N="; cin >> N;
	s = M * N;
	int* A = new int[s];
	int* B = new int[N];
	for (z = 1; z <= s; z++) 
	{
		cin >> A[z];
	}
	
	w = 0; p = 1; m = 999999;
	for (i = 1; i <= N; i++) {
		for (z = i; z <= s; z += N) {
			p*= A[z];
		}
		B[i] = p;
		p = 1;
	}
	for (i = 1; i <= N; i++) {
		if (B[i] < m) {
			m = B[i];
			w = i;
		}
	}
	cout << "nomer" << w << endl;
	cout << "proizvedenie" << w << "stolbca " << m << endl;
	return 0;
}
