using namespace std;

#include <iostream>

int main() 
{
	int N, M, i, s, x, k, t;
	double w;
	cout << "M="; cin >> M;
	cout << "N="; cin >> N;
	int C[M][N];

for (i = 0; i < M; i++)
{
for (j = 0; j < N; j++)
{
cin >> C[i][j];
}
cout << endl;
}
	s = M * N;
	double* A = new double[s];
	double* B = new double[N];

	for (k = 1; k <= s; k++) 
	{
		cin >> A[k];
	}
	x = 0; t = 0; w = 0;
	for (i = 1; i <= N; i++) {
		for (k = i; k <= s; k += N) {
			t += A[k];
		}
		w = t / M;
		B[i] = w;
		t = 0;
		w = 0;
	}
	for (i = 1; i <= N; i++) {
		cout  << i  << B[i] << endl;
	}
	for (i = 1; i <= N; i++) {
		for (k = i; k <= s; k += N) {
			if (A[k] > B[i]) { x++; }
		}
		cout << "V stolbce - " << i << endl;
		cout <<"kollichestvo - " << x << endl;
		x = 0;
	}

	return 0;

}
